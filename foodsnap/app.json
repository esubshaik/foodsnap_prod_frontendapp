{
  "expo": {
    "name": "Foodsnap",
    "slug": "foodeye",
    "version": "1.0.0",
    "scheme": "myapp",
    "androidStatusBar": {
      "barStyle": "dark-content"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "plugins": [
      "expo-router",
      "expo-camera"
    ],
    "owner": "codewithesub",
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "9a74c053-51ce-4647-96c0-e3d8d923f47e"
      }
    },
    "android": {
      "googleServicesFile": "./google-services.json",
      "permissions": [
        "android.permission.CAMERA",
        "android.permission.RECORD_AUDIO"
      ],
      "package": "com.codewithesub.foodeye"
    }
  }
}
